#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d831bc29230 .scope module, "tb_half_adder" "tb_half_adder" 2 3;
 .timescale 0 0;
v0x5d831bc3dd60_0 .var "a", 0 0;
v0x5d831bc3de20_0 .var "b", 0 0;
v0x5d831bc3def0_0 .net "cout", 0 0, L_0x5d831bc3e220;  1 drivers
v0x5d831bc3dff0_0 .net "sum", 0 0, L_0x5d831bc3e0c0;  1 drivers
S_0x5d831bc293c0 .scope module, "uut" "half_adder" 2 12, 3 1 0, S_0x5d831bc29230;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "sum";
    .port_info 1 /OUTPUT 1 "carry";
    .port_info 2 /INPUT 1 "a";
    .port_info 3 /INPUT 1 "b";
L_0x5d831bc3e0c0 .functor XOR 1, v0x5d831bc3dd60_0, v0x5d831bc3de20_0, C4<0>, C4<0>;
L_0x5d831bc3e220 .functor AND 1, v0x5d831bc3dd60_0, v0x5d831bc3de20_0, C4<1>, C4<1>;
v0x5d831bbf1ba0_0 .net "a", 0 0, v0x5d831bc3dd60_0;  1 drivers
v0x5d831bc3da60_0 .net "b", 0 0, v0x5d831bc3de20_0;  1 drivers
v0x5d831bc3db20_0 .net "carry", 0 0, L_0x5d831bc3e220;  alias, 1 drivers
v0x5d831bc3dbf0_0 .net "sum", 0 0, L_0x5d831bc3e0c0;  alias, 1 drivers
    .scope S_0x5d831bc29230;
T_0 ;
    %vpi_call 2 20 "$dumpfile", "public/vcd/wave_half_adder.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5d831bc29230 {0 0 0};
    %vpi_call 2 23 "$display", "\012Testing 1-bit Half Adder\012" {0 0 0};
    %vpi_call 2 24 "$display", "|%-5s|%-5s|%-5s|%-5s|%-5s|", "Time", "a", "b", "sum", "cout" {0 0 0};
    %vpi_call 2 25 "$display", "|-----|-----|-----|-----|-----|" {0 0 0};
    %vpi_call 2 27 "$monitor", "|%-5d|%-5b|%-5b|%-5b|%-5b|", $time, v0x5d831bc3dd60_0, v0x5d831bc3de20_0, v0x5d831bc3dff0_0, v0x5d831bc3def0_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d831bc3dd60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d831bc3de20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d831bc3dd60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d831bc3de20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d831bc3dd60_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d831bc3de20_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d831bc3dd60_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d831bc3de20_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 35 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb/tb_half_adder.v";
    "./src/half_adder.v";
