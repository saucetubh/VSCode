#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5841b0457970 .scope module, "tb_gray_to_bin" "tb_gray_to_bin" 2 4;
 .timescale -9 -12;
v0x5841b046e4b0_0 .net "bin", 2 0, L_0x5841b046eb90;  1 drivers
v0x5841b046e570_0 .var "gray", 2 0;
v0x5841b046e640_0 .var/i "i", 31 0;
S_0x5841b0457b00 .scope module, "uut" "gray_to_bin" 2 8, 3 1 0, S_0x5841b0457970;
 .timescale -9 -12;
    .port_info 0 /OUTPUT 3 "binary";
    .port_info 1 /INPUT 3 "graycode";
L_0x5841b046e710 .functor BUF 1, L_0x5841b046e800, C4<0>, C4<0>, C4<0>;
L_0x5841b046e8f0 .functor XOR 1, L_0x5841b046e9b0, L_0x5841b046eaa0, C4<0>, C4<0>;
L_0x5841b046ed00 .functor XOR 1, L_0x5841b046edc0, L_0x5841b046eeb0, C4<0>, C4<0>;
v0x5841b0457ce0_0 .net *"_ivl_0", 0 0, L_0x5841b046e710;  1 drivers
v0x5841b046dc40_0 .net *"_ivl_10", 0 0, L_0x5841b046ed00;  1 drivers
v0x5841b046dd20_0 .net *"_ivl_14", 0 0, L_0x5841b046edc0;  1 drivers
v0x5841b046dde0_0 .net *"_ivl_16", 0 0, L_0x5841b046eeb0;  1 drivers
v0x5841b046dec0_0 .net *"_ivl_3", 0 0, L_0x5841b046e800;  1 drivers
v0x5841b046dff0_0 .net *"_ivl_4", 0 0, L_0x5841b046e8f0;  1 drivers
v0x5841b046e0d0_0 .net *"_ivl_7", 0 0, L_0x5841b046e9b0;  1 drivers
v0x5841b046e1b0_0 .net *"_ivl_9", 0 0, L_0x5841b046eaa0;  1 drivers
v0x5841b046e290_0 .net "binary", 2 0, L_0x5841b046eb90;  alias, 1 drivers
v0x5841b046e370_0 .net "graycode", 2 0, v0x5841b046e570_0;  1 drivers
L_0x5841b046e800 .part v0x5841b046e570_0, 2, 1;
L_0x5841b046e9b0 .part v0x5841b046e570_0, 2, 1;
L_0x5841b046eaa0 .part v0x5841b046e570_0, 1, 1;
L_0x5841b046eb90 .concat8 [ 1 1 1 0], L_0x5841b046ed00, L_0x5841b046e8f0, L_0x5841b046e710;
L_0x5841b046edc0 .part L_0x5841b046eb90, 1, 1;
L_0x5841b046eeb0 .part v0x5841b046e570_0, 0, 1;
    .scope S_0x5841b0457970;
T_0 ;
    %vpi_call 2 16 "$dumpfile", "public/vcd/wave_gray_to_bin.vcd" {0 0 0};
    %vpi_call 2 17 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5841b0457970 {0 0 0};
    %vpi_call 2 19 "$display", "\012Testing 3-bit Gray to Binary Converter\012" {0 0 0};
    %vpi_call 2 20 "$display", "|%-5s|%-8s|%-8s|%-8s|%-8s|", "Time", "Gray", "Binary", "Gray(dec)", "Bin(dec)" {0 0 0};
    %vpi_call 2 21 "$display", "|-----|--------|--------|--------|--------|" {0 0 0};
    %vpi_call 2 23 "$monitor", "|%-5d|%-8b|%-8b|%-8d|%-8d|", $time, v0x5841b046e570_0, v0x5841b046e4b0_0, v0x5841b046e570_0, v0x5841b046e4b0_0 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5841b046e640_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5841b046e640_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5841b046e640_0;
    %parti/s 3, 0, 2;
    %store/vec4 v0x5841b046e570_0, 0, 3;
    %delay 10000, 0;
    %load/vec4 v0x5841b046e640_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5841b046e640_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %delay 10000, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
